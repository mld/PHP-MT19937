{
  "name": "mld/mersenne-twister",
  "homepage": "https://github.com/mld/mersenne-twister",
  "description": "PHP implementation of the MT19937 algorithm",
  "authors": [
    {
      "name": "Leigh T",
      "homepage": "https://github.com/lt",
      "role": "Original author"
    },
    {
      "name": "Micke LÃ¶fstrand",
      "homepage": "https://github.com/mld",
      "role": "Maintainer"
    }
  ],
  "keywords": [
    "mersenne",
    "twister",
    "prng",
    "rng",
    "random"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MLD\\MersenneTwister\\": "lib",
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^5.6 || ^7.4 || ^8.4"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "rector/rector": "^2.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": [
      "pest"
    ],
    "rector": [
      "rector process src --dry-run"
    ],
    "rector:fix": [
      "rector process src"
    ],
    "phpstan": [
      "phpstan analyse src --level max"
    ]
  }
}
